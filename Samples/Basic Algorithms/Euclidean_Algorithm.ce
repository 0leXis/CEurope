//Алгоритм Евклида (нахождение НОД 2 чисел)
простірімен Проект1
    програма
        ПисатиРядок "Введите число А";
        ціле А = ЧитатиРядок;
        ПисатиРядок "Введите число Б";
        ціле Б = ЧитатиРядок;
        
        поки( А != 0 & Б != 0)
            якщо( А > Б)
                А = А % Б;
            інакше
                Б = Б % А;
                
        ПисатиРядок "Ответ: " + (А + Б);
    кінець  
кінець